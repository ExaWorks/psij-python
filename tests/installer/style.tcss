    height: 20;
}

#key-request-url {
    margin-top: 1;
    margin-left: 2;
    margin-bottom: 1;
    background: #000000;
    color: #808080;
    padding-top: 1;
    padding-bottom: 1;
    padding-left: 2;
    padding-right: 2;
}

#key-request-dialog #status-label {
    margin-top: 1;
    margin-left: 2;
}

.form-col {
    width: auto;
    margin-right: 1;
    padding-bottom: 1;
}

.test-job-indicator {
    width: 4;
    margin-left: 1;
    margin-right: 1;
}

.job-progress-row {
    height: 1;
}

#test-job-errors {
    margin-top: 1;
    margin-bottom: 1;
}

.test-job-label {
    color: #ffffff;
    margin-right: 2;
}

.test-job-status {
    width: 6;
}

#test-jobs-dialog .status-succeeded {
    text-style: bold;
    color: #00a040;
    padding-left: 2;
}

#test-jobs-dialog .status-failed {
    color: #ff0000;
    margin-left: 0;
    margin-right: 0;
