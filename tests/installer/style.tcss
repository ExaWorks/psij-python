#test-jobs-dialog Button {
    margin-left: 1;
}

#method-status {
    margin-top: 2;
    color: #b04040;
}

App.small #method-status {
    margin-top: 1;
}

#method-preview {
    border: none;
    padding: 0;
    background: #0f2040;
    padding-left: 1;
    padding-right: 1;
    height: 3;
}

#method-preview:disabled {
    background-tint: 0%;
